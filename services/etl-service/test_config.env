# ETL Service - Configuración de Testing

# Servicio
ETL_SERVICE_NAME=etl-test
ETL_SERVICE_PORT=9000
ETL_ENV=development
ETL_LOG_LEVEL=INFO

# Base de datos de prueba (ajustar según tu configuración)
ETL_DB_HOST=localhost
ETL_DB_PORT=3306
ETL_DB_USER=root
ETL_DB_PASSWORD=root
ETL_DB_DATABASE=test_db

# LLM para análisis (opcional para testing)
ETL_LLM_PROVIDER=openai
ETL_LLM_API_KEY=sk-test-key-for-demo
ETL_LLM_MODEL=gpt-4o-mini

# KB local para testing
ETL_KB_LOCAL_PATH=/tmp/chroma_etl_test

# Configuración ETL
ETL_ENABLED=true
ETL_BATCH_SIZE=100
ETL_QUALITY_THRESHOLD=0.7
